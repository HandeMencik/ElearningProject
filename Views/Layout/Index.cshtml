
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
@Html.Action("PartialHead", "Layout")
<body class="fix-header fix-sidebar card-no-border">
    @Html.Action("PartialPreLoader", "Layout")
    <div id="main-wrapper">
        @Html.Action("PartialNavbar", "Layout")
        @Html.Action("PartialSidebar", "Layout")
        <div class="page-wrapper">
            <div class="container-fluid">
                @Html.Action("PartialPageRowTitle", "Layout")

                @RenderBody()
            </div>
            @Html.Action("PartialFooter", "Layout")
        </div>
    </div>
    @Html.Action("PartialScript", "Layout")
</body>
</html>